{
	"version": 1,
	"permissions": {
		"fs": {
			"rules": [
				"deny /etc/*",
				"allow all"
			]
		},
		"net": {
			"rules": [
				"deny google.com",
				"deny www.google.com",
				"allow all"
			]
		},
		"exec": {
			"rules": [
				"allow /bin/echo",
				"allow ./tests/helpers/child_process/*",
				"deny all"
			]
		}
	}
}