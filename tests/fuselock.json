{
	"version": 1,
	"permissions": {
		"fs": {
			"order": "allow,deny",
			"allow": [
				"**/*"
			],
			"deny": [
				"/etc/*"
			]
		},
		"net": {
			"order": "allow,deny",
			"allow": [
				"*"
			],
			"deny": [
				"google.com",
				"www.google.com"
			]
		},
		"exec": {
			"order": "deny,allow",
			"deny": [
				"**/*"
			],
			"allow": [
				"/bin/echo",
				"./tests/helpers/child_process/*"
			]
		}
	}
}